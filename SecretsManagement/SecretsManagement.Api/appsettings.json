{
  "Settings": {
    "AppDataBaseSetting": {
      "DefaultServer": "{defaultserver}",
      "DefaultPort": "{defaultport}",
      "DefaultDatabase": "{defaultdatabase}",
      "DefaultDatabaseUser": "{defaultdatabaseuser}",
      "DefaultDatabasePass": "{defaultdatabasepass}",
      "DefaultAppSchema": "{defaultappschema}",
      "DefaultmicroserviceSchema": "{defaultmicroserviceschema}"
    },
    "AppEmailSetting": {
      "EmailFrom": "{emailFrom}"
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}
